{
  "name": "@your-username/react-starter",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "description": "A starter kit for creating distributed React component libraries.",
  "author": {
    "name": "Tamer Agaoglu",
    "email": "farerock@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tamert/react-starter.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "cleanPackage": "json -I -f ./package.json -e 'delete this.author && delete this.repository && delete this.scripts && delete this.devDependencies && delete this.sideEffects'",
    "build": "webpack --config webpack.config.prod.js",
    "prepublishOnly": "yarn run build && && cleanPackage && git reset --hard",
    "start": "webpack-dev-server --config ./webpack.config.dev.js"
  },
  "peerDependencies": {
    "react": "^16.10.1",
    "react-dom": "^16.10.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-modules-commonjs": "^7.6.0",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.6.0",
    "@babel/runtime-corejs3": "^7.6.0",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "core-js": "^3.2.1",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "json": "^9.0.6",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.1",
    "react-dom": "^16.10.1",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.1",
    "regenerator-runtime": "^0.13.3",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.1",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "sideEffects": [
    "**/*.css",
    "**/*.scss"
  ]
}
